{
  "name": "ektest",
  "version": "0.1.2",
  "description": "Light weight simple testing library with Electron and Puppeteer support",
  "keywords": [
    "Javascript",
    "Test",
    "Jest",
    "Mocha",
    "Electron",
    "Puppeteer",
    "E2E Testing"
  ],
  "license": "MIT",
  "author": "Ajit Kumar",
  "type": "module",
  "main": "index.js",
  "bin": {
    "ektest": "./index.js"
  },
  "files": [
    "index.js",
    "config.js",
    "index.d.ts",
    "webpack.config.js",
    "lib/"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "npm pack && node test.js",
    "test:electron": "npm pack && node test-electron.js",
    "test:webapp": "npm pack && node test-webapp.js"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "puppeteer": "^24.27.0"
  },
  "peerDependencies": {
    "puppeteer": ">=23.0.0"
  },
  "peerDependenciesMeta": {
    "puppeteer": {
      "optional": true
    }
  }
}